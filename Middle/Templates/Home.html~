<script type="text/javascript">
	$(document).ready(function() {
		$('body').scrollspy({target:'#navbar_su'});
		$('[data-spy="scroll"]').each(function () {
  			var $spy = $(this).scrollspy('refresh')
		})
		$('.nav_taggle').mouseover(function() {
			$('.nav_taggle').find('ul').hide()
			$(this).find('ul').show()
			console.log($(this))
		 })
	})

	</script>
